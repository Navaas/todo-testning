 => {\r\n    const savedTodos = localStorage.getItem(\"todos\");\r\n    if (savedTodos) {\r\n      const parsedTodos = JSON.parse(savedTodos);\r\n      setTodos(parsedTodos);\r\n    }\r\n  }, []); // Denna useEffect körs bara en gång, vid första renderingen\r\n\r\n  // Spara todos i localStorage vid varje ändring av todos arrayen\r\n  useEffect(() => {\r\n    if (todos.length > 0) {\r\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\r\n    } else {\r\n      localStorage.removeItem(\"todos\"); // Töm localStorage när listan är tom\r\n    }\r\n  }, [todos]);\r\n\r\n  const handleAddTodo = (newTodoText: string) => {\r\n    const newTodo: Todo = {\r\n      id: Date.now(), // Genererar ett unikt ID\r\n      text: newTodoText,\r\n    };\r\n    setTodos((prevTodos) => [...prevTodos, newTodo]);\r\n  };\r\n\r\n  const handleDelete = (todoIdToDelete: number) => {\r\n    const updatedTodos = todos.filter((todo) => todo.id !== todoIdToDelete);\r\n    setTodos(updatedTodos);\r\n  };\r\n\r\n  const handleEdit = (todoId: number) => {\r\n    setEditingTodoId(todoId);\r\n  };\r\n\r\n  const handleSaveEdit = (newTodoText: string) => {\r\n    const updatedTodos = todos.map((todo) =>\r\n      todo.id === editingTodoId ? { ...todo, text: newTodoText } : todo\r\n    );\r\n    setTodos(updatedTodos);\r\n    setEditingTodoId(null);\r\n  };\r\n\r\n  const handleCancelEdit = () => {\r\n    setEditingTodoId(null);\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Header />\r\n      <div className=\"bg-[url('https://wallpapers.com/images/high/white-marble-background-5w2vm7osht89hx9s.webp')] bg-cover bg-center bg-opacity-50 w-full h-screen flex flex-col items-center justify-center\">\r\n        <h1 className=\"text-4xl md:text-6xl mb-10\">Att-göra-lista</h1>\r\n        <div className=\"flex flex-col w-full p-2 md:w-1/3 gap-4 \">\r\n          <div className=\"flex flex-col items-center bg-white p-4 w-full rounded-md shadow-xl\">\r\n            <TodoForm onSubmit={handleAddTodo} />\r\n          </div>\r\n          <div className=\"flex flex-col bg-white p-4 w-full rounded-md shadow-xl\">\r\n            <h2 className=\"font-bold\">Att göra:</h2>\r\n            <hr className=\"mb-4\" />\r\n            {todos.length === 0 ? (\r\n              <p>Listan är tom</p>\r\n            ) : (\r\n              <div className=\"flex flex-col gap-4 mb-4\">\r\n                {todos.map((todo) => (\r\n                  <div\r\n                    key={todo.id}\r\n                    className=\"p-2 flex items-center bg-gray-200 justify-between shadow-xl rounded-md hover:bg-gray-300\"\r\n                  >\r\n                    {editingTodoId === todo.id ? (\r\n                      <EditTodo\r\n                        todo={todo.text}\r\n                        onSave={handleSaveEdit}\r\n                        onCancel={handleCancelEdit}\r\n                      />\r\n                    ) : (\r\n                      <>\r\n                        <span className=\"\">{todo.text}</span>\r\n                        <div className=\"flex gap-2\">\r\n                          <button\r\n                            onClick={() => handleEdit(todo.id)}\r\n                            className=\"p-2 text-black rounded\"\r\n                            data-testid=\"edit-button\"\r\n                          >\r\n                            <svg\r\n                              xmlns=\"http://www.w3.org/2000/svg\"\r\n                              fill=\"none\"\r\n                              viewBox=\"0 0 24 24\"\r\n                              strokeWidth=\"1.5\"\r\n                              stroke=\"currentColor\"\r\n                              className=\"size-6\"\r\n                            >\r\n                              <path\r\n                                strokeLinejoin=\"round\"\r\n                                d=\"m16.862 4.487 1.687-1.688a1.875 1.875 0 1 1 2.652 2.652L10.582 16.07a4.5 4.5 0 0 1-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 0 1 1.13-1.897l8.932-8.931Zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0 1 15.75 21H5.25A2.25 2.25 0 0 1 3 18.75V8.25A2.25 2.25 0 0 1 5.25 6H10\"\r\n                              />\r\n                            </svg>\r\n                          </button>\r\n                          <DeleteButton onClick={() => handleDelete(todo.id)} />\r\n                        </div>\r\n                      </>\r\n                    )}\r\n                  </div>\r\n                ))}\r\n              </div>\r\n            )}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default App;\r\n"],"names":["useEffect","useState","DeleteButton","EditTodo","TodoForm","Header","App","todos","setTodos","editingTodoId","setEditingTodoId","savedTodos","localStorage","getItem","parsedTodos","JSON","parse","length","setItem","stringify","removeItem","handleAddTodo","newTodoText","newTodo","id","Date","now","text","prevTodos","handleDelete","todoIdToDelete","updatedTodos","filter","todo","handleEdit","todoId","handleSaveEdit","map","handleCancelEdit","div","className","h1","onSubmit","h2","hr","p","onSave","onCancel","span","button","onClick","data-testid","svg","xmlns","fill","viewBox","strokeWidth","stroke","path","strokeLinejoin","d"],"mappings":";AAAA,SAASA,SAAS,EAAEC,QAAQ,QAAQ,QAAQ;AAC5C,OAAOC,kBAAkB,yCAAyC;AAClE,OAAOC,cAAc,qCAAqC;AAC1D,OAAOC,cAAc,iCAAiC;AACtD,OAAOC,YAAY,WAAW;AAO9B,SAASC;IACP,MAAM,CAACC,OAAOC,SAAS,GAAGP,SAAiB,EAAE;IAC7C,MAAM,CAACQ,eAAeC,iBAAiB,GAAGT,SAAwB;IAElE,uDAAuD;IACvDD,UAAU;QACR,MAAMW,aAAaC,aAAaC,OAAO,CAAC;QACxC,IAAIF,YAAY;YACd,MAAMG,cAAcC,KAAKC,KAAK,CAACL;YAC/BH,SAASM;QACX;IACF,GAAG,EAAE,GAAG,4DAA4D;IAEpE,gEAAgE;IAChEd,UAAU;QACR,IAAIO,MAAMU,MAAM,GAAG,GAAG;YACpBL,aAAaM,OAAO,CAAC,SAASH,KAAKI,SAAS,CAACZ;QAC/C,OAAO;YACLK,aAAaQ,UAAU,CAAC,UAAU,qCAAqC;QACzE;IACF,GAAG;QAACb;KAAM;IAEV,MAAMc,gBAAgB,CAACC;QACrB,MAAMC,UAAgB;YACpBC,IAAIC,KAAKC,GAAG;YACZC,MAAML;QACR;QACAd,SAAS,CAACoB,YAAc;mBAAIA;gBAAWL;aAAQ;IACjD;IAEA,MAAMM,eAAe,CAACC;QACpB,MAAM